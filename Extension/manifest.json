{
   "name":"HackWestern",
   "description":"Determine semantics of youtube reviews and provide summary",
   "version": "1.0",
   "manifest_version": 3,
   "content_scripts": [
      {
         "matches": [
            "<all_urls>"
         ],
         "js": ["popup.js"]
      }
   ],
   "action": {
      "default_title": "popup",
      "default_icon": "wai hollow.png",
      "default_popup": "popup.html"
   },
   "permissions": [
      "tabs",
      "activeTab", 
      "webNavigation"
      ],
      "host_permissions": [
         "https://www.youtube.com/*"
         ]
   }
